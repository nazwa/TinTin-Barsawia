#CLASS {mowienie} OPEN

#ACTION {^%1 {mowi|krzyczy|wrzeszczy|mamroczy|pyta|nuci|mruczy|odpowiada|oznajmia|wola|spiewa|dudni|pokrzykuje|harczy|syczy} do {ciebie|siebie|}:%4$}
{
	#echo {<afa>%1 %2 do %3:<bcf>%4<099>};
	#format {hour_format} {%t} {%H:%M:%S};
	#format {log_day_format} {${nazwa_krainy}_logs/tells/%t.log} {%d-%m-%Y};
	#line log $log_day_format {[$hour_format] <afa>%1 %2 do %3:<bcf>%4<099>};
	#line log log.log {[$hour_format] <afa>%1 %2 do %3:<bcf>%4<099>};
	#unvariable hour_format;
	#unvariable log_day_format;
	#line GAG
}
{4}

#ACTION {^%1 przedstawia sie jako:$}
{
	#line gag;
	#echo {<afa>%1 przedstawia sie jako:<099>};
	#variable {opis_postaci} {%1};
	#variable {przedstawianie} {1}
}
{5}

#ACTION {^%1 {mowi|krzyczy|wrzeszczy|pyta|nuci|mamroczy|mruczy|bulgocze|odpowiada|oznajmia|wola|spiewa|dudni|pokrzykuje|harczy|syczy} do %3:%4$}
{
	#echo {<afa>%1 %2 do %3:<bcf>%4<099>};
	#format {hour_format} {%t} {%H:%M:%S};
	#format {log_day_format} {${nazwa_krainy}_logs/tells/%t.log} {%d-%m-%Y};
	#line log $log_day_format {[$hour_format] <afa>%1 %2 do %3:<bcf>%4<099>};
	#line log log.log {[$hour_format] <afa>%1 %2 do %3:<bcf>%4<099>};
	#unvariable hour_format;
	#unvariable log_day_format;
	#line GAG
}
{5}

#ACTION {^%1 {mowi|mamroczy|krzyczy|wrzeszczy|pyta|nuci|mruczy|bulgocze|odpowiada|oznajmia|wola|spiewa|dudni|pokrzykuje|harczy|syczy}{ z oddali| z calych sil| marzycielskim tonem| z irytacja w glosie| z usmiechem| cichym szeptem|}:%4$}
{
	#echo {<afa>%1 %2%3:<bcf>%4<099>};
	#format {hour_format} {%t} {%H:%M:%S};
	#format {log_day_format} {${nazwa_krainy}_logs/tells/%t.log} {%d-%m-%Y};
	#line log $log_day_format {[$hour_format] <afa>%1 %2%3:<bcf>%4<099>};
	#line log log.log {[$hour_format] <afa>%1 %2%3:<bcf>%4<099>};
	#unvariable hour_format;
	#unvariable log_day_format;
	#line GAG
}
{5}

#ACTION {^W twym umysle dzwieczy jego glos: %1$}
{
	#echo {<fad>W twym umysle dzwieczy jego glos:<bff> %1<099>};
	#format {hour_format} {%t} {%H:%M:%S};
	#format {log_day_format} {${nazwa_krainy}_logs/tells/%t.log} {%d-%m-%Y};
	#line log $log_day_format {[$hour_format] <fad>W twym umysle dzwieczy jego glos:<bff> %1<099>};
	#line log log.log {[$hour_format] <fad>W twym umysle dzwieczy jego glos:<bff> %1<099>};
	#unvariable hour_format;
	#unvariable log_day_format;
	#line GAG
}
{5}

#ACTION {^{Mowisz|Krzyczysz} do %2: %3$}
{
	#echo {<afa>%1 do %2:<bff> %3<099>};
	#format {hour_format} {%t} {%H:%M:%S};
	#format {log_day_format} {${nazwa_krainy}_logs/tells/%t.log} {%d-%m-%Y};
	#line log $log_day_format {[$hour_format] <afa>%1 do %2:<bff> %3<099>};
	#line log log.log {[$hour_format] <afa>%1 do %2:<bff> %3<099>};
	#unvariable hour_format;
	#unvariable log_day_format;
	#line GAG
}
{5}

#ACTION {^{Mowisz|Krzyczysz}: %2$}
{
	#line GAG;
	#echo {<afa>%1:<bff> %2<099>};
	#format {hour_format} {%t} {%H:%M:%S};
	#format {log_day_format} {${nazwa_krainy}_logs/tells/%t.log} {%d-%m-%Y};
	#line log $log_day_format {[$hour_format] <afa>%1:<bff> %2<099>};
	#line log log.log {[$hour_format] <afa>%1:<bff> %2<099>};
	#unvariable hour_format;
	#unvariable log_day_format
}
{5}

#ALIAS {/znane}
{
	#foreach {$znane[]} {name}
	{
		#echo {<afa>$name<faa>|<efa> $znane[$name]<099>}
	};
	#unvariable name
}
{5}

#ALIAS {pow}
{
	#format rpl {%n} {%0.};
	powiedz $rpl;
	#unvariable rpl
}
{5}

#EVENT {RECEIVED LINE}
{
	#if {$przedstawianie == 1}
	{
		#line gag;
		#echo {<afa>%0<099>};
		#variable {nazwa_postaci} {%0};
		#replace nazwa_postaci {.} {};
		#class mowienie open;
		#variable {znane[${nazwa_postaci}][opis]} {$opis_postaci};
		#map get roomvnum numer_pokoju;
		#variable {znane[${nazwa_postaci}][pokoj]} {$numer_pokoju};
		#unvariable numer_pokoju;
		#unvariable nazwa_postaci;
		#unvariable przedstawianie;
		#unvariable opis_postaci;
		#class mowienie close;
		#class mowienie write mowienie.tin
	}
}

